{
  "name": "product-details-demo",
  "version": "1.0.0",
  "description": "A basic product details app based on Vue and Vanilla JS",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server.js",
    "build": "rimraf dist && NODE_ENV=production webpack --progress",
    "test": "jest --coverage",
    "test:dev": "jest --watchAll --coverage",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.mynisum.com/plal/product-details-demo.git"
  },
  "keywords": [
    "product",
    "details",
    "vue"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/App.vue",
      "!**/node_modules/**"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "author": "Prateek Lal",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.2",
    "vue": "^2.6.11",
    "vue-icon": "^2.1.1",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "expect": "^25.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.0.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.0.1",
    "node-sass": "^4.13.1",
    "opn": "^6.0.0",
    "sass-loader": "^13.2.0",
    "sinon": "^8.1.1",
    "style-loader": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-jest": "^4.0.0-beta.2",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0"
  }
}
